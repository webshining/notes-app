    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERBMkFEOTIwNkI1OEM2NkUKUldSdXhsaHJJTmtxMnFxazdxK1Q4b0M4ck9CZUtMeEtyb283a0QrVUE3cFJ2aUVZOEh4TmZyYTcK",
      "endpoints": [
        "https://github.com/webshining/notes-app/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "nsis",
    "icon": [
      "icons/icon.ico"
